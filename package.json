{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "A template for building minified and compressed isomorphic React",
  "homepage": "https://github.com/xnerhu/react-ssrreadme",
  "author": "Miko≈Çaj Palkiewicz <xnerhu@gmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node scripts/dev.js",
    "build": "cross-env NODE_ENV=production concurrently \"rimraf ./build\" \"webpack --config webpack.config.client.js\" \"webpack --config webpack.config.server.js\"",
    "stats": "cross-env NODE_ENV=production BUNDLE_STATS=enabled concurrently \"mkdir build\" \"webpack --profile --json --config webpack.config.client.js > build/stats.json\"",
    "start": "node ./build/server/main.js",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/helmet": "0.0.44",
    "@types/nodemon": "^1.19.0",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "4.1.19",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-hot-middleware": "^2.16.5",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "cache-loader": "^4.1.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "file-type": "12.3.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "nodemon": "^1.19.4",
    "prettier": "1.18.2",
    "rimraf": "^3.0.0",
    "terser": "^4.3.9",
    "terser-webpack-plugin": "^2.1.3",
    "ts-loader": "^6.2.0",
    "typescript": "3.6.4",
    "typescript-plugin-styled-components": "^1.4.3",
    "webpack": "4.41.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.10.2",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "mobx": "^5.14.2",
    "mobx-react-lite": "^1.5.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.15",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.0-beta.8-groupsizefix"
  }
}
